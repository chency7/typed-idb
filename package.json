{
  "name": "typed-idb",
  "version": "1.0.2",
  "description": "类型安全的IndexedDB操作库，支持Web Workers和可视化分析",
  "main": "./dist/cjs/index.cjs.js",
  "module": "./dist/esm/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.esm.js",
      "require": "./dist/cjs/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "logo.svg"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepublishOnly": "npm test && npm run lint",
    "release": "standard-version",
    "start:example": "vite examples --port 3000",
    "clean": "rimraf node_modules",
    "docs:generate": "typedoc && node docs/build/process-api.js",
    "docs:build": "pnpm docs:generate && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:dev": "pnpm docs:generate && vitepress dev docs"
  },
  "dependencies": {
    "comlink": "^4.4.0",
    "esbuild": "^0.25.0",
    "eventemitter3": "^5.0.1",
    "fs-extra": "^11.3.0",
    "idb": "^7.1.1",
    "immer": "^10.0.2",
    "lodash.clonedeep": "^4.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.7.3",
    "vitepress-theme-demoblock": "^3.0.7",
    "xstate": "^4.37.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/jest": "^29.5.14",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vue/compiler-sfc": "^3.3.4",
    "core-js": "^3.32.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.33.2",
    "glob": "^11.0.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.37.1",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.27.9",
    "typedoc-plugin-frontmatter": "^1.2.1",
    "typedoc-plugin-localization": "^3.0.6",
    "typedoc-plugin-markdown": "^4.4.2",
    "vite": "^4.5.9",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "alias": {
    "vitepress": {
      "esm": "vitepress/esm"
    }
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chency7/typed-idb.git"
  },
  "keywords": [
    "indexeddb",
    "typescript",
    "web-worker",
    "database",
    "analytics",
    "type-safe",
    "async",
    "browser-storage",
    "web-storage"
  ],
  "author": "chency7 <chency7@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chency7/typed-idb/issues"
  },
  "homepage": "https://github.com/chency7/typed-idb#readme"
}
